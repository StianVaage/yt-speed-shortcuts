{
  "manifest_version": 3,
  "name": "YouTube Speed Control",
  "version": "1.0.0",
  "description": "Control YouTube playback speed with custom keyboard shortcuts. Easy-to-use popup interface for setting your preferred keys.",
  "author": "gaffel",
  "homepage_url": "https://github.com/stianvaage/yt-speed-shortcuts",
  "permissions": ["storage"],
  "icons": {
    "16": "assets/icon16.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "YouTube Speed Control",
    "default_icon": {
      "16": "assets/icon16.png",
      "48": "assets/icon48.png",
      "128": "assets/icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["content.js"]
    }
  ]
}
